[pull]
	rebase = true
	tags = true
[fetch]
	prune = true
	pruneTags = true
[push]
	default = current
	followTags = true
[status]
	showUntrackedFiles = all
[rebase]
	instructionFormat = [%an] - %s
[alias]
	pf = push --force-with-lease
	co = checkout
[user]
	name = Jake Bottrall
	email = 75273444+bottrall@users.noreply.github.com
[core]
	editor = vim
	hooksPath = ~/.config/git/hooks
[credential "https://github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
